    <h3>PRICE</h3>
    <div class="price_wrap">
      <div class="price_fee">
        <div class="price_t">
          <img src="__PUBLIC__/home/img/mobile/PayAsYouGo.png" alt="">
        </div>
        <div class="price_fee_box">
          <foreach name="billing_data"  item="vo" key="k" >
            <p><{$vo.billing_plan_details}></p>
            <if condition="($k+1) neq count($billing_data)">
              <p>or</p>
            </if>
          </foreach>
        </div>
        <div class="price_i">
          <img src="__PUBLIC__/home/img/mobile/pay.png" alt="">
        </div>
      </div>
      <!--<div class="price_i">-->
      <!--<img src="__PUBLIC__/home/img/mobile/pay.png" alt="">-->
      <!--</div>-->
      <div class="price_credit">
        <div class="price_credit_wrap">
          <div class="credit_l">
            <div class="credit_l_img">
              <img src="__PUBLIC__/home/img/mobile/creditl.png" alt="">
            </div>
            <div class="credit_wrap">
              <p>Gain Points</p>
              <ul style="height:4rem;overflow-y: scroll">
                <foreach name="gaincredit" item="vo">
                  <li>
                    <span class="credit_descr"><{$vo.content}></span>
                    <span class="credit_countr"><{$vo.credit_point}></span>
                  </li>
                </foreach>
              </ul>
            </div>
          </div>
          <div class="credit_r">
            <div class="credit_r_img">
              <img src="__PUBLIC__/home/img/mobile/creditl.png" alt="">
            </div>
            <div class="credit_wrap">
              <p>Lose Points</p>
              <ul style="height:4rem;overflow-y: scroll">
                <foreach name="losecredit" item="vo">
                  <li>
                    <span class="credit_descr"><{$vo.content}></span>
                    <span class="credit_countr"><{$vo.credit_point}></span>
                  </li>
                </foreach>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
