    <div class="zezao"></div>
    <div class="faqBox">
      <div class="faq">
        <span class="close">X</span>
        <p class="faq-title">FAQ</p>
        <ul>
          <foreach name="faq"  item="vo" key="k" >
            <if condition="$k eq 0">
              <li class="navbar">
                <p>Extra information</p>
                <p>Q:<{$vo.faq_question}></p>
                <p>A:<{$vo.faq_answer}></p>
              </li>
              <else />
              <li class="navbar">
                <p>Q:<{$vo.faq_question}></p>
                <p>A:<{$vo.faq_answer}></p>
              </li>
            </if>
          </foreach>

        </ul>
        <div class="faq_nav">
          <p>FAQ</p>
          <ul>
            <li>><a href="" class="Goindex">1</a></li>
            <li>><a href="" class="Goindex">2</a></li>
            <li>><a href="" class="Goindex">3</a></li>
            <li>><a href="" class="Goindex">4</a></li>
            <li>><a href="" class="Goindex">5</a></li>
          </ul>
        </div>
      </div>
    </div>
    <div class="termBox">
      <div class="term">
        <span class="close1">X</span>
        <p class="term-title">Terms&Condtions</p>
        <ul>
          <foreach name="creditcondition"  item="vo" key="k" >
            <if condition = "$vo.credit_type eq 1">
              <p>Gaincredit: [<{$vo.credit_point}>] <{$vo.content}></p>
              <else/>
              <p>Losecredit: [<{$vo.credit_point}>] <{$vo.content}></p>
            </if>
          </foreach>
        </ul>
      </div>
    </div>
